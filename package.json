{
  "name": "gatsby-source-mozaik",
  "version": "1.0.0",
  "description":
    "Gatsby source plugin for building static websites using Mozaik CMS as a data source.",
  "keywords": [
    "data",
    "gatsby",
    "gatsby-plugin",
    "mozaik",
    "mozaikio",
    "graphql",
    "source"
  ],
  "homepage": "https://github.com/mozaikio/gatsby-source-mozaik",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozaikio/gatsby-source-mozaik.git"
  },
  "bugs": {
    "url": "https://github.com/mozaikio/gatsby-source-mozaik/issues"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "author": "peter.balazs@mozaik.io",
  "deprecated": false,
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "jest": "^22.2.2"
  }
}
